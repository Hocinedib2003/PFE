{
  "name": "diagnostic",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "package": "build",
    "dist": "electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "electron": "^36.2.1",
    "electron-builder": "^26.0.12"
  },
  "build": {
    "appId": "com.yourcompany.diagnostic",
    "productName": "Diagnostic LMA",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "package.json",
      {
        "from": "public",
        "to": "app/public",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "Models",
        "to": "app/Models",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "target": "nsis",
      "icon": "public/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
